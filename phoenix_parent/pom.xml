					<version>9.3.2.v20150730</version>
				<!-- jetty 8.0 配置方式				  
				<configuration>
				    <scanIntervalSeconds>5</scanIntervalSeconds>
				    <webApp>
				      <contextPath>/phoenix_web</contextPath>
				    </webApp>
				    <connectors>
				       <connector implementation="org.eclipse.jetty.server.nio.SelectChannelConnector">
				          <port>8888</port>
				          <maxIdleTime>60000</maxIdleTime>
				       </connector>
				     </connectors>
				</configuration> -->
				<configuration>
				<scanIntervalSeconds>5</scanIntervalSeconds>
					<httpConnector>
						<port>8888</port>
					</httpConnector>
					<webAppConfig>
